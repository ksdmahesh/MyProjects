<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent">
  <LinearLayout
      style="@style/layout"
      android:layout_height="wrap_content">
    <LinearLayout
        style="@style/layoutTextbox">
      <LinearLayout
          style="@style/layoutRow">
        <EditText
            android:longClickable="false"
            android:id="@+id/QText"
            style="@style/textbox" />
      </LinearLayout>
      <LinearLayout
          style="@style/layoutRow">
        <LinearLayout
            style="@style/layoutColumn"
            android:layout_weight="0.8">
          <TextView
              android:id="@+id/AText"
              style="@style/textbox" />
        </LinearLayout>
        <LinearLayout
            style="@style/layoutColumn"
            android:layout_weight="0.25">
          <Button
              android:text="←"
              android:id="@+id/BackSpace"
              style="@style/buttons" />
        </LinearLayout>
        <LinearLayout
            style="@style/layoutColumn"
            android:layout_weight="0.25">
          <Button
              android:text="↑"
              android:id="@+id/Up"
              style="@style/buttons" />
        </LinearLayout>
        <LinearLayout
            style="@style/layoutColumn"
            android:layout_weight="0.25">
          <Button
              android:text="↓"
              android:id="@+id/Down"
              style="@style/buttons" />
        </LinearLayout>
      </LinearLayout>
      <RadioGroup
          style="@style/radioButtonGroup">
        <RadioButton
            style="@style/radioButton"
            android:checked="true"
            android:text="Deg"
            android:id="@+id/Degree" />
        <RadioButton
            style="@style/radioButton"
            android:text="Rad"
            android:id="@+id/Radient" />
        <RadioButton
            style="@style/radioButton"
            android:text="Grad"
            android:id="@+id/Gradient" />
        <Button
            android:text="Clr"
            style="@style/button"
            android:id="@+id/Clr" />
      </RadioGroup>
    </LinearLayout>
    <ScrollView
        style="@style/layout">
      <LinearLayout
          style="@style/layoutButton">
        <LinearLayout
            style="@style/layoutRow">
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="|"
                style="@style/textView" />
            <Button
                android:text="&amp;"
                style="@style/buttons"
                android:id="@+id/AND" />
          </LinearLayout>
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="~"
                style="@style/textView" />
            <Button
                android:text="^"
                style="@style/buttons"
                android:id="@+id/XOR" />
          </LinearLayout>
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="D"
                style="@style/textView" />
