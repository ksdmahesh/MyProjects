          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                style="@style/textView" />
            <Button
                android:text="="
                style="@style/buttons"
                android:id="@+id/Solve" />
          </LinearLayout>
        </LinearLayout>
      </LinearLayout>
    </ScrollView>
  </LinearLayout>
</LinearLayout>