            <TextView
                android:text="Rect"
                style="@style/textView" />
            <Button
                android:text="a+ib"
                style="@style/buttons"
                android:id="@+id/APolar" />
          </LinearLayout>
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="∠"
                style="@style/textView" />
            <Button
                android:text="i"
                style="@style/buttons"
                android:id="@+id/i" />
          </LinearLayout>
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="Hcf"
                style="@style/textView" />
            <Button
                android:text="Lcm"
                style="@style/buttons"
                android:id="@+id/LCM" />
          </LinearLayout>
          <LinearLayout
              style="@style/layoutColumn">
            <TextView
                android:text="Mod"
                style="@style/textView" />
            <Button
                android:text="Abs"
                style="@style/buttons"
                android:id="@+id/Abs" />
          </LinearLayout>
        </LinearLayout>
        <LinearLayout
            style="@style/layoutRow">
          <LinearLayout
